{
  "name": "node-fct",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start_old": "pm2 start ./bin/www --watch",
    "dev": "cross-env NODE_ENV=development supervisor --harmony index.js",
    "check": "cross-env NODE_ENV=check supervisor  --harmony index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production pm2 start index.js --node-args='--harmony' --name 'node_fct --port 8001'",
    "stop": "cross-env NODE_ENV=production pm2 stop index.js --name 'node_fct --port 8001'",
    "restart": "cross-env NODE_ENV=production pm2 restart index.js --node-args='--harmony' --name 'node_fct --port 8001'"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "config-lite": "^2.1.0",
    "connect-history-api-fallback": "^1.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "pm2": "^3.2.2",
    "supervisor": "^0.12.0"
  }
}
